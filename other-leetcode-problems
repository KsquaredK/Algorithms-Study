// TwoSum: find indices in items in array that add up target amount
let nums = [2,7,11,15]
let target = 9

const twoSum = (nums, target) => {
    let arr = [];
    for (x of nums){
        for (y of nums) {
            if (nums[x] + nums[y] === target)
            arr.push(x, y)
        }
        return arr;
    }
   console.log(twoSum[arr]);
}